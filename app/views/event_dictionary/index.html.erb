<% @title = 'Event Dictionaries' %>
<div class="page-header">
  <h1>
    <%= @title %>
    <%= link_to "Create Event Dictionary", new_event_dictionary_path, class: 'btn btn-mini btn-primary' %>
  </h1>
</div>

<%= render :partial => "layouts/search", locals: { object: :event_dictionary, path: event_dictionary_index_path } %>
<%= render partial: 'layouts/per_page', locals: { object: :event_dictionary, per_page: 25 } %>

<div class="center" id="ajax_loader"><%= image_tag "contour/ajax-loader.gif" %></div>
<div id="event_dictionary" class="index-content"></div>

